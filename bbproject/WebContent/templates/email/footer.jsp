<div id="footerBlock-<%=templateEnum %>" class="blockSelectable" 
		style="display:<%=((templateVisible) ? "" : "none") %>">
	<div style="width:94%;padding:1.25rem;background:#ffffff;overflow:hidden;height:1%;">
		<div style="width:100%;text-align:center;">
			<div id="footerName-<%=templateEnum %>" style="font-size: 1rem;color:#000000">
			</div>
			<div style="clear:both;line-height:.0625rem">&nbsp;
			</div>
			<div id="footerDescription-<%=templateEnum %>" style="font-size: 1.375rem;font-weight:lighter;padding-bottom:1.875rem;font-style:italic">
			</div>
		</div>
	</div>

	<div id="footerBlock-<%=templateEnum %>SortUpBtn" class="sortUp">
		<img src="images/bench/sortUp.png" alt="" />
	</div>
	<div id="footerBlock-<%=templateEnum %>TrashBtn" class="trashBlock">
		<img src="images/bench/trashIcon.png" alt="" />
	</div>
	<div id="footerBlock-<%=templateEnum %>SortDownBtn" class="sortDown" style="left:.125rem">
		<img src="images/bench/sortDown.png" alt="" />
	</div> 
	
</div>
